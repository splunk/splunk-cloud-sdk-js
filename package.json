{
  "name": "@splunk/ssc-client",
  "version": "0.1.2",
  "description": "Splunk SSC SDK for Javascript",
  "main": "splunk.js",
  "scripts": {
    "build": "babel src -d .",
    "clean": "find src/ -name '*.js' | sed 's/src\\//./' | xargs rm",
    "codecov": "yarn cover && codecov -f ./coverage/lcov.info -F unit",
    "codecov:integration": "yarn cover:integration && codecov -f ./coverage-integration/lcov.info -F integration",
    "cover": "NODE_ENV=coverage nyc --reporter=lcov yarn test && yarn run cover:report",
    "cover:integration": "NODE_ENV=coverage nyc --reporter=lcov --report-dir coverage-integration yarn test:integration && yarn run cover:report",
    "cover:report": "nyc report",
    "lint": "eslint src/** examples/** ",
    "lint:fix": "eslint src/** examples/** --fix",
    "pretest": "yarn lint",
    "test": "mocha test/unit",
    "test:integration": "mocha --timeout=10000 --require babel-polyfill test/integration",
    "start:stubserver": "docker run --name=stubserver -d -p 8882:8882 137462835382.dkr.ecr.us-west-1.amazonaws.com/ssc-sdk-shared-stubby:latest",
    "stop:stubserver": "docker rm -f stubserver",
    "apidocs": "jsdoc -r -c jsdoc.json -d doc/api/ --pedantic",
    "start:swagger": "docker run --name=swagger -d -p 80:8080 swaggerapi/swagger-ui",
    "stop:swagger": "docker rm -f swagger",
    "example": "node examples/*.js"
  },
  "author": "Splunk, Inc.",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "is-guid": "^0.0.2",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.11.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "js-base64": "^2.4.3",
    "rxjs": "^5.5.8"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "**/**.js"
    ],
    "exclude": [
      ".tmp",
      "ci",
      "coverage",
      "dist",
      "node_modules",
      "test",
      "tmp",
      ".eslintrc.js"
    ]
  }
}
