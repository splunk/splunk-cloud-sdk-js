{
  "name": "ssc-client-js",
  "version": "0.0.1",
  "description": "Splunk SSC SDK for Javascript",
  "main": "src/index.js",
  "scripts": {
    "codecov": "codecov",
    "cover": "NODE_ENV=coverage nyc --reporter=lcov yarn test",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "build": "webpack --display minimal",
    "pretest": "yarn lint",
    "test": "yarn build && mocha --require babel-core/register",
    "start:stubserver": "docker run --name=stubserver -d -p 8882:8882 137462835382.dkr.ecr.us-west-1.amazonaws.com/ssc-sdk-shared:latest",
    "stop:stubserver": "docker rm -f stubserver",
    "apidocs": "jsdoc -r -c jsdoc.json -d doc/api/",
    "start:swagger": "docker run --name=swagger -d -p 80:8080 swaggerapi/swagger-ui",
    "stop:swagger": "docker rm -f swagger"
  },
  "author": "Splunk, Inc.",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "babel-eslint": "^7",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "js-yaml": "^3.11.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "stubby": "^4.0.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "js-base64": "^2.4.3",
    "rxjs": "^5.5.8"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false,
    "exclude": [
      ".tmp",
      "test",
      "node_modules"
    ]
  }
}
