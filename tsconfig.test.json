{
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "lib": [
      "es5",
      "es2015.iterable",
      "es2015.collection",
      "es2015.promise",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "dom"
    ],
    "types": ["node"]
  },
  "include": [
    "test/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "test/**/*.d.ts",
    "test/**/*.js",
    "test/**/*.js.map"
  ]
}
